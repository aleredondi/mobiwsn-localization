package remote_interfaces.protocol;

import java.rmi.Remote;
import java.rmi.RemoteException;

public interface NetworkAddress extends Remote {
		
		public String getString() throws RemoteException;
		
		public short getAddressPart( int i) throws RemoteException;
		
		public int intValue() throws RemoteException;
		
		public boolean equals( NetworkAddress anotherAddress ) throws RemoteException;

}
