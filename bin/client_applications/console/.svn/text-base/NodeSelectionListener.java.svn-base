
package client_applications.console;

import java.util.Vector;


/**
 *
 * @author Davide Roveran
 */
public interface NodeSelectionListener 
{
    
    /**
     * Signaled when a DrawNode object is selected on the graph
     * 
     * @param selectedNode DrawNode selected
     */
    void nodeSelected(DrawNode selectedNode);

    
    /**
     * Signaled when a DrawNode is unselected and no other
     * node is selected
     */
    void nodeUnselected();
    
    
    public DrawNode getSelectedNode();
    
    public Vector<DrawNode> getGroup();
}
