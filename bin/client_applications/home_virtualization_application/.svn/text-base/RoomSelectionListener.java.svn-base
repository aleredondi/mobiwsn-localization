package client_applications.home_virtualization_application;

import client_applications.home_virtualization_application.DrawRoom;


/**
 *
 * @author Davide Roveran
 */
public interface RoomSelectionListener 
{
    
    /**
     * Signaled when a DrawNode object is selected on the graph
     * 
     * @param selectedNode DrawNode selected
     */
    void roomSelected(DrawRoom selectedRoom);

    
    /**
     * Signaled when a DrawNode is unselected and no other
     * node is selected
     */
    void roomUnselected();
}
