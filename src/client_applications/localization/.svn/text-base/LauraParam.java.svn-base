package client_applications.localization;

public class LauraParam {
	
	boolean raw_on;
	int weights;
	double alpha_c, alpha_u, cloud_thr;
	
	public LauraParam(){
		
	}
	public LauraParam(boolean raw_on, int weights, double alpha_c, double alpha_u, double cloud_thr){
		this.raw_on = raw_on;
		this.weights = weights;
		this.alpha_c = alpha_c;
		this.alpha_u = alpha_u;
		this.cloud_thr = cloud_thr;
	}

	public boolean isRaw_on() {
		return raw_on;
	}

	public void setRaw_on(boolean rawOn) {
		raw_on = rawOn;
	}

	public int getWeights() {
		return weights;
	}

	public void setWeights(int weights) {
		this.weights = weights;
	}

	public double getAlpha_c() {
		return alpha_c;
	}

	public void setAlpha_c(double alphaC) {
		alpha_c = alphaC;
	}

	public double getAlpha_u() {
		return alpha_u;
	}

	public void setAlpha_u(double alphaU) {
		alpha_u = alphaU;
	}
	public void setCloud_thr(double cloudThr) {
		cloud_thr = cloudThr;
		System.out.println("cloud_thr = " +cloud_thr);
	}
	public double getCloud_thr() {
		return cloud_thr;
	}
	

	
}
